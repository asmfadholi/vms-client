{
  "name": "vms-client",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "NODE_ENV=production nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "deploy:staging": "gcloud app deploy app-staging.yaml --project staging-vms",
    "routing:staging": "gcloud app deploy dispatch-staging.yaml --project staging-vms",
    "deploy:prod": "gcloud app deploy app.yaml --project outing-apps",
    "routing:prod": "gcloud app deploy dispatch.yaml --project outing-apps",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/auth-next": "^5.0.0-1608461079.ae1e5db",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/pwa": "^3.0.2",
    "ant-design-vue": "^1.6.5",
    "core-js": "^3.6.5",
    "dot-qs": "^0.2.0",
    "dotenv": "^8.2.0",
    "nuxt": "^2.14.6",
    "vue-lazyload": "^1.3.3",
    "vue-qrcode-reader": "^2.3.16"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxt/types": "^2.14.6",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-config-typescript": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.0",
    "eslint": "^7.10.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "fibers": "^5.0.0",
    "husky": "^4.3.0",
    "jest": "^26.5.0",
    "lint-staged": "^10.4.0",
    "sass": "^1.30.0",
    "sass-loader": "^13.1.0",
    "ts-jest": "^26.4.1",
    "vue-jest": "^3.0.4"
  },
  "peerDependencies": {
    "dotenv": "^8.2.0"
  }
}
